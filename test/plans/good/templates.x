/*
Templates are similar to formats but can have variable sizes
*/

List template<typeA>
{
    .buffer typeA default;
    .size u32;
}

List constructor<>
{
    build(u8,u8,List
}

entry function<> -> u32
{
    // constructor
    create myListA <- List<u8>();

    // normal
    create myListB <- List<u8>();
}